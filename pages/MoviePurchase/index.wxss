.moviepurchasebox .moviepurchase .moviepurchase-bannerbox .moviepurasecity {
  display: flex;
  height: 80rpx;
  justify-content: space-between;
  padding: 0 32rpx;
  line-height: 80rpx;
  background: #fff;
}
.moviepurchasebox .moviepurchase .moviepurchase-bannerbox .moviepurasecity .moviepurasecity-left image {
  display: inline-block;
  width: 52rpx;
  height: 52rpx;
}
.moviepurchasebox .moviepurchase .moviepurchase-bannerbox .moviepurasecity .moviepurasecity-left text {
  display: inline-block;
  margin-left: 20rpx;
  font-weight: bold;
}
.moviepurchasebox .moviepurchase .moviepurchase-bannerbox .moviepurasecity .moviepurasecity-right i {
  font-size: 48rpx;
  color: #333;
  margin-right: 24rpx;
}
.moviepurchasebox .moviepurchase .moviepurchase-bannerbox .moviepurchase-banner-cover {
  position: absolute;
  width: 100%;
  height: 250px;
  filter: blur(20px);
  z-index: -1;
}
.moviepurchasebox .moviepurchase .moviepurchase-bannerbox .moviepurchase-banner {
  position: relative;
  height: 500rpx;
}
.moviepurchasebox .moviepurchase .moviepurchase-bannerbox .moviepurchase-banner .moviepurchase-banner-swiper {
  position: absolute;
  height: 345rpx;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
}
.moviepurchasebox .moviepurchase .moviepurchase-bannerbox .moviepurchase-banner .moviepurchase-banner-swiper .moviepurchase-banner-item {
  width: 230rpx;
  height: 345rpx;
  margin-right: 15px;
}
.moviepurchasebox .moviepurchase .moviepurchase-bannerbox .moviepurchase-banner .moviepurchase-banner-swiper .moviepurchase-banner-item .moviepurchase-banner-slide {
  width: 230rpx;
  height: 345rpx;
  transform: scale(0.8);
  transition: all .3s ease;
}
.moviepurchasebox .moviepurchase .moviepurchase-bannerbox .moviepurchase-banner .moviepurchase-banner-swiper .moviepurchase-banner-item .moviepurchase-banner-slide-active {
  transform: scale(1);
}
.moviepurchasebox .moviepurchase .moviepurchase-bannerbox .moviepurchase-card {
  padding: 32rpx 56rpx;
  margin-bottom: 20rpx;
  border-bottom: 2rpx solid #dedede;
  background: #fff;
}
.moviepurchasebox .moviepurchase .moviepurchase-bannerbox .moviepurchase-card .moviepurchasecard-tit {
  height: 60rpx;
  line-height: 60rpx;
}
.moviepurchasebox .moviepurchase .moviepurchase-bannerbox .moviepurchase-card .moviepurchasecard-tit span {
  float: left;
  color: #191a1b;
  font-size: 36rpx;
  font-weight: bold;
}
.moviepurchasebox .moviepurchase .moviepurchase-bannerbox .moviepurchase-card .moviepurchasecard-tit b {
  float: right;
  font-size: 36rpx;
  font-style: italic;
  color: #ffb232;
}
.moviepurchasebox .moviepurchase .moviepurchase-bannerbox .moviepurchase-card .moviepurchasecard-sub {
  display: flex;
  justify-content: space-between;
  line-height: 64rpx;
  text-align: center;
  color: #797d82;
}
