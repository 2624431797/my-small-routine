.mationlistbox {
  background: #fff;
}
.mationlistbox .mationlist {
  background: #fff;
}
.mationlistbox .mationlist .mationlist-content {
  position: relative;
}
.mationlistbox .mationlist .mationlist-content .mationlist-item {
  display: flex;
  justify-content: space-between;
  padding: 20rpx;
}
.mationlistbox .mationlist .mationlist-content .mationlist-item .mationlist-list {
  width: 70%;
  margin-right: 36rpx;
}
.mationlistbox .mationlist .mationlist-content .mationlist-item .mationlist-list text {
  display: block;
  font-size: 36rpx;
  font-weight: bold;
  margin-bottom: 10rpx;
}
.mationlistbox .mationlist .mationlist-content .mationlist-item .mationlist-list view:nth-of-type(1) {
  font-size: 24rpx;
  color: #333;
  text-indent: 48rpx;
  line-height: 40rpx;
}
.mationlistbox .mationlist .mationlist-content .mationlist-item .mationlist-list .mationlist-subion {
  display: flex;
  justify-content: space-between;
  font-size: 24rpx;
  color: #ccc;
  margin-top: 10rpx;
}
.mationlistbox .mationlist .mationlist-content .mationlist-item .mationlist-img {
  width: 200rpx;
  height: 220rpx;
  border-radius: 20rpx;
  overflow: hidden;
}
.mationlistbox .mationlist .mationlist-content .mationlist-image image {
  height: 85rpx;
}
.mationlistbox .mationlist .list-no-more {
  width: 100%;
  height: 100rpx;
  color: #666;
  text-align: center;
  line-height: 100rpx;
}
