html, body, dl, ul, ol, li, dt, dd, form, img, fieldset, table, tr, td, h1, h2,
h3, h4, h5, h6, p, legend {
  padding: 0;
  margin: 0;
}

image, fieldset {
  border: none;
}

page {
  font-size: 14px;
  color: #333;
  font-family: "微软雅黑";
  background: #fff;
}

image {
  vertical-align: middle;
  display: block;
  border: none;
}

.clear:after {
  content: "";
  clear: both;
  display: block;
  height: 0;
  overflow: hidden;
  visibility: hidden;
}

input {
  outline: none;
  border: none;
}

page {
  height: 100%;
}

image {
  width: 100%;
  height: 100%;
}

table {
  border-collapse: collapse;
}
