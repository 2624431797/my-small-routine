<view class="mationbannerbox">
  <swiper 
    class="mationbanner"
    indicator-dots
    indicator-color="#fff"
    indicator-active-color="#ff5f16"
    autoplay
    interval="4000"
    easing-function="easeInOutCubic"
  >
    <block wx:for="{{bannerlist}}" wx:key="id">
      <swiper-item>
        <image src="{{item.imgUrl}}"></image>
      </swiper-item>
    </block>
  </swiper>
</view>